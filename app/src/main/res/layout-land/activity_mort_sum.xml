<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView21"
            android:layout_below="@+id/textView20"
            android:layout_alignEnd="@+id/textView20" />

        <TextView
            android:text="@string/monthly_property_Tax"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            android:layout_above="@+id/textView12"
            android:layout_alignParentStart="true"
            android:layout_marginStart="17dp" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView23"
            android:layout_below="@+id/textView22"
            android:layout_alignStart="@+id/textView21" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="39dp"
            android:id="@+id/textView3"
            android:layout_below="@+id/textView"
            android:layout_alignEnd="@+id/textView4"
            android:layout_marginEnd="33dp" />

        <TextView
            android:text="@string/yearly_HOA"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="33dp"
            android:id="@+id/textView20"
            android:layout_below="@+id/textView3"
            android:layout_alignEnd="@+id/textView3" />

        <TextView
            android:text="@string/total_HOA_Paid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView22"
            android:layout_marginTop="26dp"
            android:layout_below="@+id/textView21"
            android:layout_alignStart="@+id/textView20" />

        <Button
            android:text="@string/go_Back_Home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="sendMessage"
            android:id="@+id/mGoBack"
            android:layout_alignBaseline="@+id/textView"
            android:layout_alignBottom="@+id/textView"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="94dp" />

        <TextView
            android:text="@string/mortgage_Summary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="51dp"
            android:id="@+id/textView"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/textView4"
            android:layout_marginStart="27dp" />

        <TextView
            android:text="@string/monthly_HI"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView2"
            android:layout_marginEnd="49dp"
            android:layout_alignBaseline="@+id/textView20"
            android:layout_alignBottom="@+id/textView20"
            android:layout_alignEnd="@+id/mGoBack" />

        <TextView
            android:text="@string/total_HI_Paid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            android:layout_marginStart="13dp"
            android:layout_above="@+id/textView12"
            android:layout_alignStart="@+id/textView2" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView19"
            android:layout_below="@+id/textView2"
            android:layout_alignStart="@+id/textView12" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            android:layout_alignBaseline="@+id/textView3"
            android:layout_alignBottom="@+id/textView3"
            android:layout_alignStart="@+id/mGoBack" />


</RelativeLayout>