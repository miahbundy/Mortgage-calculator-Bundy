<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView"
            android:layout_alignParentStart="true"
            android:layout_marginStart="40dp"
            android:layout_marginTop="39dp"
            android:id="@+id/textView3" />

        <TextView
            android:text="@string/mortgage_Summary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="51dp"
            android:id="@+id/textView"
            android:layout_alignParentTop="true"
            android:layout_alignEnd="@+id/mGoBack"
            android:textAppearance="@style/TextAppearance.AppCompat.Medium" />

        <TextView
            android:text="Monthly Tax"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView4"
            android:layout_below="@+id/textView"
            android:layout_alignStart="@+id/textView3"
            android:layout_marginTop="15dp" />

        <TextView
            android:text="Monthly HI"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView11"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="18dp"
            android:id="@+id/textView2" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            android:layout_alignBaseline="@+id/textView3"
            android:layout_alignBottom="@+id/textView3"
            android:layout_alignEnd="@+id/textView11" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView19"
            android:layout_alignBaseline="@+id/textView12"
            android:layout_alignBottom="@+id/textView12"
            android:layout_alignStart="@+id/textView2" />

        <TextView
            android:text="HOA /year"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView3"
            android:layout_alignStart="@+id/textView3"
            android:layout_marginTop="39dp"
            android:id="@+id/textView20" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/textView20"
            android:layout_alignStart="@+id/textView20"
            android:layout_marginTop="14dp"
            android:id="@+id/textView21" />

        <TextView
            android:text="Total HOA Paid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignTop="@+id/textView20"
            android:layout_centerHorizontal="true"
            android:id="@+id/textView22" />

        <TextView
            android:text="Total HI "
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView11"
            android:layout_alignBaseline="@+id/textView4"
            android:layout_alignBottom="@+id/textView4"
            android:layout_centerHorizontal="true" />

        <TextView
            android:text="TextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignBottom="@+id/textView21"
            android:layout_alignStart="@+id/textView11"
            android:id="@+id/textView23" />

        <Button
            android:text="@string/go_Back_Home"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:onClick="sendMessage"
            android:id="@+id/mGoBack"
            android:layout_below="@+id/textView23"
            android:layout_alignStart="@+id/textView12"
            android:layout_marginTop="14dp" />


</RelativeLayout>